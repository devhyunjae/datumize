{
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "semi": ["error", "never"],
        "import/prefer-default-export": false,
        "import/no-extraneous-dependencies": false,
        "react/forbid-prop-types": false,
        "function-paren-newline": 0
    }
}
